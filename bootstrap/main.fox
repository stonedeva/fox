import "std.fox"

define OP_PUSH 0
define OP_PLUS 1
define OP_MINUS 2
define OP_MULT 3
define OP_DIVI 4

func usage in
    "Options:\n" @puts
    "	-v	Show current version\n" @puts
    "	-o	Set output path\n" @puts
    "	-l	Dump lexical analysis\n" @puts
    "	-t	Dump type stack\n" @puts
    "	-h	Show help map\n" @puts 
end

func main in
    if argc 2 > do
	"Usage: ./foxc [options...] <file>\n" @eputs
	"No input file provided!\n" @eputs
	1 return
    end

    0 return
end
